/ {
	chosen {
		stdout-path = &uart1;
	};

	memory {
		reg = <0x80000000 0x20000000>;
	};

#ifdef WIFI
	bd7181_bl {
		compatible = "bd7181x-backlight";
		regulator-name = "bl-wled";
		regulator-boot-on;
		regulator-supply = "backlight";
		backlight-supply = <&wled_reg>;

		brightness {
			brightness-levels = <10 20 30 50 70 100 200 300 500 700 1000 2000 3000 4000 5000 6000 7000 8000 9000 
								10000 11000 12000 13000 14000 15000 16000 17000 18000 19000 20000 21000 22000 23000 24000 25000>;
			default-brightness-level = <15>;
		};
	};
#endif

	pxp_v4l2 {
		compatible = "fsl,imx6ul-pxp-v4l2", "fsl,imx6sx-pxp-v4l2", "fsl,imx6sl-pxp-v4l2";
		status = "okay";
	};

	
	regulators {
		compatible = "simple-bus";
		#address-cells = <1>;
		#size-cells = <0>;
		
		reg_sd1_vmmc: regulator_sd1_vmmc {
			compatible = "regulator-fixed";
			regulator-name = "wlan-en-regulator";
			regulator-min-microvolt = <1800000>;
			regulator-max-microvolt = <1800000>;
	
			/* WLAN card specific delay */
			/* startup-delay-us = <70000>; */
			/* enable-active-high; */
		};
		reg_gpio_dvfs: regulator-gpio {
			compatible = "regulator-gpio";
			regulator-min-microvolt = <1300000>;
			regulator-max-microvolt = <1400000>;
			regulator-name = "gpio_dvfs";
			regulator-type = "voltage";
			gpios = <&gpio4 13 GPIO_ACTIVE_HIGH>;
			states = <1300000 0x1 1400000 0x0>;
		};
	};
	

	codec: max98357a@0 {
		compatible = "maxim,max98357a";
		sdmode-gpios = <&gpio1 11 0>;
	};

	sound {
		compatible = "fsl,imx-max98357a-audio";
		ssi-controller = <&sai2>;
		codec = <&codec>;
	};

	keys {
		compatible = "gpio-keys";

		button_tr {
			label = "BUTTONS.T_R";
			gpios = <&gpio2 0 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_F1>;
			gpio-key,wakeup;
		};

		button_tl {
			label = "BUTTONS.T_L";
			gpios = <&gpio2 1 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_F2>;
			gpio-key,wakeup;
		};

		keyboard_tr {
			label = "BUTTONS.KB_T_R";
			gpios = <&gpio5 1 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_F4>;
			gpio-key,wakeup;
		};

		keyboard_tl {
			label = "BUTTONS.KB_T_L";
			gpios = <&gpio5 2 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_F3>;
			gpio-key,wakeup;
		};

		keyboard_br {
			label = "BUTTONS.KB_B_R";
			gpios = <&gpio5 3 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_F6>;
			gpio-key,wakeup;
		};

		keyboard_bl {
			label = "BUTTONS.KB_B_L";
			gpios = <&gpio5 4 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_F5>;
			gpio-key,wakeup;
		};
	};

	/* TBD
	leds {
		compatible = "gpio-leds";

		d16_led {
			gpios = <&gpio4 20 0>;
			linux,default-trigger = "heartbeat";
		};
	};
	*/
};

&cpu0 {
	arm-supply = <&reg_arm>;
	soc-supply = <&reg_soc>;
	dc-supply = <&reg_gpio_dvfs>;
};

&clks {
	/* ref_clk for micrel ethernet phy */
	rmii_ref_clk: rmii_ref_clk_grp {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-frequency = <25000000>;
		clock-output-names = "rmii-ref";
	};
};

&clks {
	assigned-clocks = <&clks IMX6UL_CLK_PLL4_POST_DIV>;
	assigned-clock-rates = <812851200>;
};

&flexcan1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_flexcan1>;
	status = "okay";
};

&flexcan2 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_flexcan2>;
	status = "disabled";
};

&gpc {
	fsl,cpu_pupscr_sw2iso = <0x2>;
	fsl,cpu_pupscr_sw = <0x1>;
	fsl,cpu_pdnscr_iso2sw = <0x1>;
	fsl,cpu_pdnscr_iso = <0x1>;
	fsl,ldo-bypass = <0>; /* DCDC, ldo-enable */
};

&lcdif {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_lcdif_dat
		     &pinctrl_lcdif_ctrl>;
	display = <&display0>;
	status = "okay";

	display0: display {
		bits-per-pixel = <32>;
		bus-width = <24>;
		
		gpios = <
				&gpio3 5 GPIO_ACTIVE_HIGH
				&gpio3 4 GPIO_ACTIVE_HIGH
				>;
		
		display-timings {
			native-mode = <&timing0>;
			timing0: timing0 {
				clock-frequency =<24000000>;
				hactive = <480>;
				vactive = <640>;
				hfront-porch = <10>;
				hback-porch = <8>;
				hsync-len = <32>;
				vback-porch = <13>;
				vfront-porch = <13>;
				vsync-len = <5>;
				hsync-active = <0>;
				vsync-active = <0>;
				de-active = <1>;
				pixelclk-active = <0>;
			};
		};
	};
};

&pwm2 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_pwm2>;
	status = "okay";
};

&pxp {
	status = "okay";
};

&sai2 {
        pinctrl-names = "default";
        pinctrl-0 = <&pinctrl_sai2>;
        assigned-clocks = <&clks IMX6UL_CLK_SAI2_SEL>,
                          <&clks IMX6UL_CLK_PLL4_AUDIO_DIV>,
                          <&clks IMX6UL_CLK_SAI2>;
        assigned-clock-rates = <0>, <786432000>, <12288000>;
        assigned-clock-parents = <&clks IMX6UL_CLK_PLL4_AUDIO_DIV>;
        status = "okay";
};

/* Console UART */
&uart1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart1>;
	status = "okay";
};

/* Bluetooth UART */
&uart2 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart2>;
	fsl,uart-has-rtscts;
	status = "okay";
};

/* ttymxc2 UART [GPS]*/
&uart3 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart3>;
	status = "okay";
};



/* ttymxc4 UART [ANT]*/
&uart5 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart5>;
	status = "okay";
};

/* ttymxc5 UART [GPRS] */
&uart6 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart6>;
	status = "okay";
};

&usbotg1 {
	dr_mode = "otg";
	disable-over-current;
	status = "okay";
};

&usbotg2 {
	dr_mode = "host";
	disable-over-current;
	status = "okay";
};

#ifdef WIFI
/*  Wi-Fi/BT SD Card Slot Definition */
&usdhc1 {
	compatible = "fsl,imx6ull-usdhc", "fsl,imx6sx-usdhc";
	reg = <0x02190000 0x4000>;
	interrupts = <GIC_SPI 22 IRQ_TYPE_LEVEL_HIGH>;
	clocks = <&clks IMX6UL_CLK_USDHC1>,
		 <&clks IMX6UL_CLK_USDHC1>,
		 <&clks IMX6UL_CLK_USDHC1>;
	clock-names = "ipg", "ahb", "per";
	assigned-clocks = <&clks IMX6UL_CLK_USDHC1_SEL>, <&clks IMX6UL_CLK_USDHC1>;
	assigned-clock-parents = <&clks IMX6UL_CLK_PLL2_PFD2>;
	assigned-clock-rates = <0>, <132000000>;
	fsl,tuning-step= <2>;
	status = "disabled";
	pinctrl-names = "default", "state_100mhz", "state_200mhz";
	pinctrl-0 = <&pinctrl_usdhc1>;
	pinctrl-1 = <&pinctrl_usdhc1_100mhz>;
	pinctrl-2 = <&pinctrl_usdhc1_200mhz>;
	vmmc-supply = <&reg_sd1_vmmc>;
	keep-power-in-suspend;
	enable-sdio-wakeup;
	bus-width = <4>;
	broken-cd;
	cap-power-off-card;
	status = "okay";
	#address-cells = <1>;
	#size-cells = <0>;

	wlcore: wlcore@0 {
		compatible = "ti,wl1835";
		reg = <2>;
		interrupt-parent = <&gpio1>;
		interrupts = <28 IRQ_TYPE_LEVEL_HIGH>;
	};
};
#else
&usdhc1 {
	pinctrl-names = "default", "state_100mhz", "state_200mhz";
	pinctrl-0 = <&pinctrl_usdhc1>;
	pinctrl-1 = <&pinctrl_usdhc1_100mhz>;
	pinctrl-2 = <&pinctrl_usdhc1_200mhz>;
	no-1-8-v;
	keep-power-in-suspend;
	enable-sdio-wakeup;
	cd-gpios = <&gpio4 19 GPIO_ACTIVE_LOW>;
	status = "okay";
};
#endif

&usdhc2 {
	compatible = "fsl,imx6ull-usdhc", "fsl,imx6sx-usdhc";
	reg = <0x02194000 0x4000>;
	interrupts = <GIC_SPI 23 IRQ_TYPE_LEVEL_HIGH>;
	clocks = <&clks IMX6UL_CLK_USDHC2>,
		 <&clks IMX6UL_CLK_USDHC2>,
		 <&clks IMX6UL_CLK_USDHC2>;
	clock-names = "ipg", "ahb", "per";
	assigned-clocks = <&clks IMX6UL_CLK_USDHC2_SEL>, <&clks IMX6UL_CLK_USDHC2>;
	assigned-clock-parents = <&clks IMX6UL_CLK_PLL2_PFD2>;
	assigned-clock-rates = <0>, <132000000>;
	bus-width = <8>;
	fsl,tuning-step= <2>;
	pinctrl-names = "default", "state_100mhz", "state_200mhz";
	pinctrl-0 = <&pinctrl_usdhc2_8bit>;
	pinctrl-1 = <&pinctrl_usdhc2_8bit_100mhz>;
	pinctrl-2 = <&pinctrl_usdhc2_8bit_200mhz>;
	non-removable;
	status = "okay";
};

/* NOT USED. CHECK
&i2c1 {
	clock-frequency = <400000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c1>;
	status = "okay";

};
*/

#ifdef WIFI
&i2c2 {
	clock_frequency = <100000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c2>;
	status = "okay";
	
	tsc@24 {
		compatible = "cy,cyttsp5_i2c_adapter";
		reg = <0x24>;
		interrupt-parent = <&gpio2>;
		interrupts = <2 IRQ_TYPE_EDGE_FALLING>;
		cy,adapter_id = "cyttsp5_i2c_adapter";

		cy,core {
			cy,name = "cyttsp5_core";

			cy,irq_gpio = <34>;
			cy,rst_gpio = <39>;
			cy,hid_desc_register = <1>;
			/* CY_CORE_FLAG_RESTORE_PARAMETERS */
			cy,flags = <4>;
			/* CY_CORE_EWG_NONE */
			cy,easy_wakeup_gesture = <0>;

			cy,mt {
				cy,name = "cyttsp5_mt";

				cy,inp_dev_name = "cyttsp5_mt";
				cy,flags = <0>;
				cy,abs =
					/* ABS_MT_POSITION_X, CY_ABS_MIN_X, CY_ABS_MAX_X, 0, 0 */
					<0x35 0 480 0 0
					/* ABS_MT_POSITION_Y, CY_ABS_MIN_Y, CY_ABS_MAX_Y, 0, 0 */
					0x36 0 640 0 0
					/* ABS_MT_PRESSURE, CY_ABS_MIN_P, CY_ABS_MAX_P, 0, 0 */
					0x3a 0 255 0 0
					/* CY_IGNORE_VALUE, CY_ABS_MIN_W, CY_ABS_MAX_W, 0, 0 */
					0xffff 0 255 0 0
					/* ABS_MT_TRACKING_ID, CY_ABS_MIN_T, CY_ABS_MAX_T, 0, 0 */
					0x39 0 15 0 0
					/* ABS_MT_TOUCH_MAJOR, 0, 255, 0, 0 */
					0x30 0 255 0 0
					/* ABS_MT_TOUCH_MINOR, 0, 255, 0, 0 */
					0x31 0 255 0 0
					/* ABS_MT_ORIENTATION, -127, 127, 0, 0 */
					0x34 0xffffff81 127 0 0
					/* ABS_MT_TOOL_TYPE, 0, MT_TOOL_MAX, 0, 0 */
					0x37 0 1 0 0
					/* ABS_DISTANCE, 0, 255, 0, 0 */
					0x19 0 255 0 0>;

				cy,enable_vkeys = <0>;
				cy,vkeys_x = <480>;
				cy,vkeys_y = <640>;
				cy,x_max = <480>;
				cy,y_max = <640>;
				cy,p_max = <255>;
				cy,slots_max = <32>;
			};
		};
	};
	
	pmic: bd7181x@4b {
		compatible = "rohm,bd71815";
		reg = <0x4b>;
		gpio_intr = <&gpio2 12 IRQ_TYPE_EDGE_FALLING>;
		irq_base = <0>;

		gpo {
			rohm,mode = <0x30>;	/* 0b0011_0000 all gpos with cmos output mode */
		};

		regulators {
                        buck1_reg: buck1 {
                                regulator-min-microvolt = <900000>;
                                regulator-max-microvolt = <1400000>;
                                regulator-boot-on;
                                regulator-always-on;
                                regulator-ramp-delay = <6250>;
                        };

                        buck2_reg: buck2 {
                                regulator-min-microvolt = <800000>;
                                regulator-max-microvolt = <2000000>;
                        };

                        buck3_reg: buck3 {
                                regulator-min-microvolt = <2300000>;
                                regulator-max-microvolt = <2300000>;
                                regulator-boot-on;
                                regulator-always-on;
                        };

                        buck4_reg: buck4 {
                                regulator-min-microvolt = <1350000>;
                                regulator-max-microvolt = <1350000>;
                                regulator-boot-on;
                                regulator-always-on;
                        };

                        buck5_reg: buck5 {
                                regulator-min-microvolt = <3300000>;
                                regulator-max-microvolt = <3300000>;
                                regulator-boot-on;
                                regulator-always-on;
                        };

                        ldo1_reg: ldo1 {
                                regulator-min-microvolt = <3300000>;
                                regulator-max-microvolt = <3300000>;
                                regulator-boot-on;
                                regulator-always-on;
                        };

                        ldo2_reg: ldo2 {
                                regulator-min-microvolt = <3300000>;
                                regulator-max-microvolt = <3300000>;
                                regulator-boot-on;
                                regulator-always-on;
                        };

                        ldo3_reg: ldo3 {
                                regulator-min-microvolt = <3300000>;
                                regulator-max-microvolt = <3300000>;
                        };

                        ldo4_reg: ldo4 {
                                regulator-min-microvolt = <1800000>;
                                regulator-max-microvolt = <1800000>;
                        };

                        ldo5_reg: ldo5 {
                                regulator-min-microvolt = <1800000>;
                                regulator-max-microvolt = <1800000>;
                                regulator-boot-on;
                                regulator-always-on;
                        };

                        dvref_reg: dvref {
                                regulator-boot-on;
                                regulator-always-on;
                        };

                        lpsr_reg: lpsr {
                                regulator-boot-on;
                                regulator-always-on;
                        };

                        wled_reg: wled {
                                regulator-min-microamp = <10>;
                                regulator-max-microamp = <25000>;
                                /*regulator-boot-on;*/
                        };

		};

	};

	tsl2x7x@39 {
        	compatible = "taos,tsl2572";
        	reg = <0x39>;
    	};

	/* DS1337 RTC module
	rtc@0x68 {
		compatible = "dallas,ds1337";
		reg = <0x68>;
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_rtc>;
		interrupt-parent = <&gpio5>;
		interrupts = <7 IRQ_TYPE_EDGE_FALLING>;
	};
	*/
};
#endif

&iomuxc {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_hog_1>;
	imx6ul-evk {
		pinctrl_hog_1: hoggrp-1 {
			fsl,pins = <
				/* MX6UL_PAD_CSI_HSYNC__GPIO4_IO20		0x1b0b0	User LED - Check */
				/* MX6UL_PAD_GPIO1_IO00__GPIO1_IO00	0x17059	User Button - Check */
				/* MX6UL_PAD_GPIO1_IO07__ENET2_MDC		0x1b0b0 Need to Check */ 
				/* MX6UL_PAD_GPIO1_IO06__ENET2_MDIO	0x1b0b0 Need to Check */
				/*-------------------------------BUTTONS-------------------------------*/
				MX6UL_PAD_ENET1_RX_DATA1__GPIO2_IO01	0x1b0b0 /* BUTTONS_TL*/
				MX6UL_PAD_ENET1_RX_DATA0__GPIO2_IO00	0x1b0b0 /* BUTTONS_TR*/
				/*-------------------------------GPS-----------------------------------*/
				MX6UL_PAD_UART3_RTS_B__GPIO1_IO27		0x1b0b0 /* GPS_SYSTEM_ON (INPUT)*/
				MX6UL_PAD_UART3_CTS_B__GPIO1_IO26		0x1b0b0 /* GPS_ON_OFF */
				MX6UL_PAD_UART1_CTS_B__GPIO1_IO18		0x1b0b0 /* GPS_RESET */
				MX6UL_PAD_ENET1_TX_DATA0__GPIO2_IO03	0x1b0b0 /* GPS_PWR_EN */
				MX6UL_PAD_UART1_RTS_B__GPIO1_IO19		0x1b0b0 /* GPS_BOOTSEL */
				/*-------------------------------GPRS----------------------------------*/
				MX6UL_PAD_ENET2_TX_CLK__GPIO2_IO14		0x1b0b0 /* GPRS_SWITCH */
				MX6UL_PAD_ENET2_TX_EN__GPIO2_IO13		0x1b0b0 /* GPRS_PWRKEY */
				MX6UL_PAD_ENET2_TX_DATA0__GPIO2_IO11	0x1b0b0 /* GPRS_STATUS (INPUT) */
				/*-------------------------------WiFi-----------------------------------*/
				MX6UL_PAD_UART4_RX_DATA__GPIO1_IO29		0x4001b0b0 /* WL_ENABLE */
				MX6UL_PAD_UART5_RX_DATA__GPIO1_IO31		0x1b0b0 /* WL_SWITCH */
				/*-------------------------------BLE-----------------------------------*/
				MX6UL_PAD_UART5_TX_DATA__GPIO1_IO30		0x1b0b0 /* BT_EN */
				/*-------------------------------PCAP----------------------------------*/
				MX6UL_PAD_ENET1_RX_ER__GPIO2_IO07		0x1b0b0 /* PCAP_RESET */
				MX6UL_PAD_ENET1_RX_EN__GPIO2_IO02		0x1b0b0 /* PCAP_INT */
				/*---------------------------------------------------------------------*/
				/*MX6UL_PAD_LCD_DATA17__GPIO3_IO22		0x40000000 Sample to get feedback from grpio sysfs*/
			>;
		};

		pinctrl_flexcan1: flexcan1grp{
			fsl,pins = <
				MX6UL_PAD_LCD_DATA09__FLEXCAN1_RX	0x1b020
				MX6UL_PAD_LCD_DATA08__FLEXCAN1_TX	0x1b020
			>;
		};

		pinctrl_flexcan2: flexcan2grp{
			fsl,pins = <
				MX6UL_PAD_UART2_RTS_B__FLEXCAN2_RX	0x1b020
				MX6UL_PAD_UART2_CTS_B__FLEXCAN2_TX	0x1b020
			>;
		};

		pinctrl_lcdif_dat: lcdifdatgrp {
			fsl,pins = <
				MX6UL_PAD_LCD_DATA02__LCDIF_DATA02	0x79
				MX6UL_PAD_LCD_DATA03__LCDIF_DATA03	0x79
				MX6UL_PAD_LCD_DATA04__LCDIF_DATA04	0x79
				MX6UL_PAD_LCD_DATA05__LCDIF_DATA05	0x79
				MX6UL_PAD_LCD_DATA06__LCDIF_DATA06	0x79
				MX6UL_PAD_LCD_DATA07__LCDIF_DATA07	0x79
				MX6UL_PAD_LCD_DATA10__LCDIF_DATA10	0x79
				MX6UL_PAD_LCD_DATA11__LCDIF_DATA11	0x79
				MX6UL_PAD_LCD_DATA12__LCDIF_DATA12	0x79
				MX6UL_PAD_LCD_DATA13__LCDIF_DATA13	0x79
				MX6UL_PAD_LCD_DATA14__LCDIF_DATA14	0x79
				MX6UL_PAD_LCD_DATA15__LCDIF_DATA15	0x79
				MX6UL_PAD_LCD_DATA18__LCDIF_DATA18	0x79
				MX6UL_PAD_LCD_DATA19__LCDIF_DATA19	0x79
				MX6UL_PAD_LCD_DATA20__LCDIF_DATA20	0x79
				MX6UL_PAD_LCD_DATA21__LCDIF_DATA21	0x79
				MX6UL_PAD_LCD_DATA22__LCDIF_DATA22	0x79
				MX6UL_PAD_LCD_DATA23__LCDIF_DATA23	0x79
			>;
		};

		pinctrl_lcdif_ctrl: lcdifctrlgrp {
			fsl,pins = <
				MX6UL_PAD_LCD_CLK__LCDIF_CLK		0x79
				MX6UL_PAD_LCD_ENABLE__LCDIF_ENABLE	0x79
				MX6UL_PAD_LCD_HSYNC__LCDIF_HSYNC	0x79
				MX6UL_PAD_LCD_VSYNC__LCDIF_VSYNC	0x79
				MX6UL_PAD_LCD_RESET__GPIO3_IO04		0x1b0b1
				MX6UL_PAD_LCD_DATA00__GPIO3_IO05	0x1b0b1
			>;
		};

		pinctrl_pwm2: pwm2grp {
			fsl,pins = <
				MX6UL_PAD_GPIO1_IO09__PWM2_OUT		0x110b0
			>;
		};

		pinctrl_uart1: uart1grp { /* DEBUG */
			fsl,pins = <
				MX6UL_PAD_UART1_TX_DATA__UART1_DCE_TX	0x1b0b1
				MX6UL_PAD_UART1_RX_DATA__UART1_DCE_RX	0x1b0b1
			>;
		};

		pinctrl_uart2: uart2grp { /* WLAN */
			fsl,pins = <
				MX6UL_PAD_UART2_TX_DATA__UART2_DCE_TX	0x1b0b1
				MX6UL_PAD_UART2_RX_DATA__UART2_DCE_RX	0x1b0b1
				MX6UL_PAD_UART2_CTS_B__UART2_DCE_CTS	0x1b0b1
				MX6UL_PAD_UART2_RTS_B__UART2_DCE_RTS	0x1b0b1
			>;
		};

		pinctrl_uart3: uart3grp { /* GPS */
